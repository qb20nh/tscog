{
  "name": "tscog",
  "version": "0.0.1",
  "description": "Generate code from and to any language",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc && ts-standard",
    "test": "jest"
  },
  "pre-commit": [
    "pnpm run lint",
    "pnpm run build",
    "pnpm run test"
  ],
  "keywords": [
    "Code generation",
    "codegen",
    "cog",
    "gocog",
    "metaprogramming"
  ],
  "author": "qb20nh",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "pre-commit": "^1.2.2",
    "ts-jest": "^29.0.5",
    "ts-standard": "^12.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
